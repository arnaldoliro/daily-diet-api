generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id    Int     @id @default(autoincrement())
  name  String
  email String @unique
  meals meals[]
}

model meals {
  id          Int      @id @default(autoincrement())
  name        String
  description String?
  createAt       DateTime @default(now())
  onDiet      Boolean
  userId      Int
  user  user?  @relation(fields: [userId], references: [id])
}
